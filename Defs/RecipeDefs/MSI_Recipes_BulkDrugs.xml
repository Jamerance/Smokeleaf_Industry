<?xml version="1.0" encoding="utf-8" ?>
<RecipeDefs>

	<!--=== Smokeleaf Joint Recipe ===-->
	
	<RecipeDef>
		<defName>BulkRollSmokeleafJoint</defName>
		<label>make smokeleaf joints (bulk)</label>
		<description>Roll more joints at a time.</description>
		<jobString>rolling smokeleaf joints</jobString>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<workAmount>3000</workAmount>
		<workSkill>Crafting</workSkill>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>SmokeleafBuds</li>
					</thingDefs>
				</filter>
				<count>20</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>SmokeleafBuds</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<SmokeleafJoint>10</SmokeleafJoint>
		</products>
		<recipeUsers>
			<li>DrugLab</li>
			<li>PoweredCultivationBench</li>
		</recipeUsers>
		<researchPrerequisite>IndustrialSmokeleafCultivation</researchPrerequisite>
	</RecipeDef>
	
